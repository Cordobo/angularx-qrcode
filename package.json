{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@ubirch/angularx-qrcode",
  "version": "12.0.0",
  "author": {
    "name": "Beate Fiss <beate.fiss@ubirch.com> - based on angularx-qrcode from Andreas Jacob <andreas@cordobo.de (http://cordobo.com/)",
    "url": "https://ubirch.com"
  },
  "description": "Simple QRCode module generator for Angular12 and Ionic 4/5 using node-qrcode - based on the great work on angularx-qrcode package of Andreas Jacob <andreas@cordobo.de (http://cordobo.com/)",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angularx-qrcode --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angularx-qrcode && npm publish --access public && cd ../../",
    "start:demo": "ng serve demo-app --open",
    "dev:demo": "lite-server --baseDir=\"dist/demo-app\"",
    "build:demo": "npm run build && ng build demo-app",
    "build:demo:prod": "npm run build && ng build demo-app --configuration production"
  },
  "dependencies": {
    "qrcode": "^1.4.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/qrcode": "1.3.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.8",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lite-server": "^2.6.1",
    "ng-packagr": "^12.2.5",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "rxjs": "~6.6.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  },
  "keywords": [
    "angularx-qrcode",
    "angular-qrcode",
    "ng-qrcode",
    "ngx-qrcode",
    "angular",
    "angular12",
    "ionic",
    "ionic4",
    "ionic5",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubirch/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/ubirch/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/ubirch/angularx-qrcode#readme"
}
