{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "angularx-qrcode",
  "version": "12.0.0",
  "author": {
    "name": "Beate Fiss <beate.fiss@ubirch.com>",
    "url": "https://ubirch.com"
  },
  "description": "Simple QRCode module generator for Angular12 and Ionic 4/5 using node-qrcode - based on the great work on angularx-qrcode package of Andreas Jacob <andreas@cordobo.de (http://cordobo.com/)",
  "license": "MIT",
  "ngPackage": {
    "dest": "../../dist/angularx-qrcode",
    "lib": {
      "entryFile": "src/public-api.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json"
  },
  "peerDependencies": {
    "@angular/core": "~12.2.13",
    "qrcode": "1.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ubirch/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/ubirch/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/ubirch/angularx-qrcode#readme"
}
