{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "angularx-qrcode",
  "version": "10.0.8",
  "author": {
    "name": "Andreas Jacob <andreas@cordobo.de>",
    "url": "http://cordobo.com/"
  },
  "description": "Simple QRCode module generator for Angular4/5/6/7/8/9/10+ and Ionic 3/4/5 using node-qrcode",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angularx-qrcode --prod",
    "test": "ng test",
    "lint": "ng lint",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angularx-qrcode && npm publish && cd ../../"
  },
  "dependencies": {
    "qrcode": "1.4.2"
  },
  "peerDependencies": {
    "@angular/core": ">= 10.0.0 < 11.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular/cli": "~10.0.2",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/qrcode": "1.3.4",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "rxjs": "~6.5.5",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.3"
  },
  "keywords": [
    "angularx-qrcode",
    "angular-qrcode",
    "ng-qrcode",
    "ngx-qrcode",
    "angular",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "ionic",
    "ionic3",
    "ionic4",
    "ionic5",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cordobo/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/cordobo/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/cordobo/angularx-qrcode#readme"
}
