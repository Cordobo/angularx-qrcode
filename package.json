{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "angularx-qrcode",
  "version": "2.3.1",
  "author": {
    "name": "Andreas Jacob <andreas@cordobo.de>",
    "url": "http://cordobo.com/"
  },
  "description": "Simple QRCode module generator for Angular4/5/6/7/8/9+ and Ionic 3/4 using node-qrcode",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "lint": "tslint 'src/**/*.ts' --fix",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angularx-qrcode && npm publish && cd ../../"
  },
  "dependencies": {
    "@angular/common": ">= 9.0.0 < 10.0.0",
    "qrcode": "1.4.4"
  },
  "peerDependencies": {
    "@angular/common": ">= 9.0.0 < 10.0.0",
    "@angular/core": ">= 9.0.0 < 10.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@types/node": "^12.11.1",
    "@types/qrcode": "1.3.4",
    "codelyzer": "5.2.2",
    "ng-packagr": "9.1.1",
    "rimraf": "3.0.2",
    "rxjs": "~6.5.4",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  },
  "keywords": [
    "angularx-qrcode",
    "angular-qrcode",
    "ng-qrcode",
    "ngx-qrcode",
    "angular",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "ionic",
    "ionic3",
    "ionic4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cordobo/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/cordobo/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/cordobo/angularx-qrcode#readme"
}
